@if (TempData["ErrorMassege"] != null ||
				TempData["InfoMassege"] != null ||
				TempData["WarningMassege"] != null ||
				TempData["SuccessMassege"] != null)
{

	if (TempData["SuccessMassege"] != null)
	{
		<script>
			$(document).ready(function () {
				ShowMessage('Success Message', '@TempData["SuccessMassege"]', 'success')
			})
		</script>
	}
	if (TempData["InfoMassege"] != null)
	{
		<script>
			$(document).ready(function () {
				ShowMessage('Info Message', '@TempData["InfoMassege"]', 'info')
			})
		</script>
	}
	if (TempData["ErrorMassege"] != null)
	{
		<script>
			$(document).ready(function () {
				ShowMessage('Error Message', '@TempData["ErrorMassege"]', 'error')
			})
		</script>
	}
	if (TempData["WarningMassege"] != null)
	{
		<script>
			$(document).ready(function () {
				ShowMessage('Warning Message', '@TempData["WarningMassege"]', 'warning')
			})
		</script>
	}

}